            }
            elseif ($StudentId -notlike "*@*.*") {
                $StudentId = "$($StudentId)@$($Script:PSGSuite.Domain)"
            }
        }
        foreach ($invId in $InvitationId) {
            try {
                if ($PSCmdlet.ShouldProcess("Revoking Guardian Invitation '$invId' for Student '$StudentId'")) {
