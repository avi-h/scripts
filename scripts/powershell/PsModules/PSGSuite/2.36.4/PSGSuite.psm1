                                    }
                                    else {
                                        Write-Verbose "Retrieved $retrieved licenses for product '$origProdId'..."
                                    }
                                    if ($Limit -gt 0 -and $total -eq $Limit) {
                                        Write-Verbose "Limit reached: $Limit"
                                        $overLimit = $true
                                    }
